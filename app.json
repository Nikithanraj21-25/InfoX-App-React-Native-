{
  "expo": {
    "name": "Cli2Con",
    "slug": "Cli2Con",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/c2c.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_logo_new.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nikithan.InfoX"
    },
    "android": {
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_final.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nikithan.InfoX"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "68eaeea0-1c98-462c-8e0e-9d2a47c7af19"
      }
    },
    "owner": "nikithan"
  }
}
