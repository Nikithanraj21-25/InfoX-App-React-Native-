{
  "version": 2,
  "builds": [
    {
      "src": "script.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/process-image",
      "dest": "/script.js"
    },
    {
      "src": "/(.*)",
      "methods": ["GET", "POST"],
      "dest": "/script.js"
    }
  ]
}
